{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "tmo-assignment",
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "projects": {
    "tmo-assignment": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/tmo-assignment",
      "sourceRoot": "apps/tmo-assignment/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/tmo-assignment",
            "index": "apps/tmo-assignment/src/index.html",
            "main": "apps/tmo-assignment/src/main.ts",
            "polyfills": "apps/tmo-assignment/src/polyfills.ts",
            "tsConfig": "apps/tmo-assignment/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/tmo-assignment/src/favicon.ico",
              "apps/tmo-assignment/src/assets"
            ],
            "styles": [
              "apps/tmo-assignment/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/tmo-assignment/src/environments/environment.ts",
                  "with": "apps/tmo-assignment/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tmo-assignment:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tmo-assignment:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tmo-assignment:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/tmo-assignment/src/test.ts",
            "polyfills": "apps/tmo-assignment/src/polyfills.ts",
            "tsConfig": "apps/tmo-assignment/tsconfig.spec.json",
            "karmaConfig": "apps/tmo-assignment/karma.conf.js",
            "assets": [
              "apps/tmo-assignment/src/favicon.ico",
              "apps/tmo-assignment/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/tmo-assignment/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/tmo-assignment/tsconfig.app.json",
              "apps/tmo-assignment/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tmo-assignment:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tmo-assignment:serve:production"
            }
          }
        }
      }
    },
    "tmo-assignment-e2e": {
      "root": "apps/tmo-assignment-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/tmo-assignment-e2e/protractor.conf.js",
            "devServerTarget": "tmo-assignment:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tmo-assignment:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/tmo-assignment-e2e/tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  }
}